{
    "identifier": "blockchain.address.viewer",
    "name": "Blockchain Address Viewer",
    "actions": [
        {
            "title": "Token",
            "icon": "link",
            "requirements": ["text"],
            "regex": "^0x[a-fA-F0-9]{40}$",
            "after": "open-url",
            "url": "https://gmgn.ai/eth/token/{popclip text}"
        },
        {
            "title": "Token",
            "icon": "link",
            "requirements": ["text"],
            "regex": "^[1-9A-HJ-NP-Za-km-z]{32,44}$",
            "after": "open-url",
            "url": "https://gmgn.ai/sol/token/{popclip text}"
        },
        {
            "title": "Wallet",
            "icon": "link",
            "requirements": ["text"],
            "regex": "^0x[a-fA-F0-9]{40}$",
            "after": "open-url",
            "url": "https://gmgn.ai/eth/address/{popclip text}"
        },
        {
            "title": "Wallet",
            "icon": "link",
            "requirements": ["text"],
            "regex": "^[1-9A-HJ-NP-Za-km-z]{32,44}$",
            "after": "open-url",
            "url": "https://gmgn.ai/sol/address/{popclip text}"
        },
        {
            "title": "Debank",
            "icon": "link",
            "requirements": ["text"],
            "regex": "^0x[a-fA-F0-9]{40}$",
            "after": "open-url",
            "url": "https://debank.com/profile/{popclip text}"
        }
    ]
} 